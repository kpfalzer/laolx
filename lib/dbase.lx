namespace dbase {
    interface Credentials {}

    class UserPassword(string @user, string @password) : Credentials {
    }

    interface Dbase {
        def connect(string url, string database, Credentials credentials) => Exception
    }

    class MySql(string @url, string @database, string @user, string @password) : Dbase {
        connect(url, database, UserPassword.new(user, password))
    }
}
